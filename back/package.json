    "@nestjs/platform-socket.io": "^7.5.2",
    "@nestjs/platform-ws": "^7.5.2",
    "@nestjs/swagger": "^4.7.9",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjs/websockets": "^7.5.2",
