    "@types/redis": "^2.8.28",
    "@types/socket.io-redis": "^1.0.26",
    "@types/supertest": "^2.0.10",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "ava": "^3.13.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^6.15.0",
